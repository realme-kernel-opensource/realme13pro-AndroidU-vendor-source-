&i2c1 {
	pinctrl-names = "i2c-state", "output-low-state";
	pinctrl-0 = <&i2c1_i2c_state>;
	pinctrl-1 = <&i2c1_output_low_state>;
};

&pio {
	i2c1_i2c_state: i2c1_sda_i2c_state {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO100__FUNC_SCL1>,
			<PINMUX_GPIO101__FUNC_SDA1>;
			bias-pull-up;
		};
	};
	i2c1_output_low_state: i2c1_sda_output_low_state {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO100__FUNC_GPIO100>,
			<PINMUX_GPIO101__FUNC_GPIO101>;
			output-low;
		};
	};
};

&i2c13 {
	status = "okay";
	clock-frequency = <400000>;

	oplus_sc2201: sc8547-sc2201@72{
		status = "okay";
		compatible = "oplus,sc2201-ufcs";
		reg = <0x72>;

		oplus,ufcs_int-gpio = <&pio 82 0>;
		oplus,ufcs_en-gpio = <&pio 115 0>;

		pinctrl-names = "ufcs_int_default", "ufcs_en_active", "ufcs_en_sleep";
		pinctrl-0 = <&ufcs_int_default>;
		pinctrl-1 = <&ufcs_en_active>;
		pinctrl-2 = <&ufcs_en_sleep>;
	};
};

&pio {
	ufcs_int_default: ufcs_int_default {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO82__FUNC_GPIO82>;
			input-enable;
			bias-pull-up;
		};
	};
	ufcs_en_active: ufcs_en_active {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO115__FUNC_GPIO115>;
			bias-pull-up;
			input-disable;
			output-high;
		};
	};
	ufcs_en_sleep: ufcs_en_sleep {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO115__FUNC_GPIO115>;
			bias-pull-down;
			input-disable;
			output-low;
		};
	};
};
